{
    "name": "learn-gulp",
    "version": "0.0.1",
    "description": "learn-gulp",
    "main": "build/index.js",
    "scripts": {
        "start:build": "npm run tsc && node build",
        "start": "node run",
        "test": "mocha src/**/*.test.ts",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run clear && tsc -w",
        "p": "npm run tsc && npm publish",
        "lint": "tslint \"src/**/*.ts\""
    },
    "dependencies": {
        "gulp": "^3.9.1",
        "gulp-clean": "^0.3.2",
        "gulp-minify-css": "^1.2.4",
        "gulp-sourcemaps": "^2.6.0",
        "gulp-uglify": "^3.0.0",
        "lodash": "^4.17.4",
        "pump": "^1.0.2",
        "tslib": "^1.6.1",
        "uglify-js": "^3.0.13"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.64",
        "@types/mocha": "^2.2.41",
        "@types/node": "^6.0.48",
        "@types/should": "^8.3.0",
        "mocha": "^3.3.0",
        "rimraf": "^2.6.1",
        "should": "^11.2.1",
        "ts-node": "^3.0.2",
        "tslint": "^5.1.0",
        "typescript": "^2.3.2"
    },
    "keywords": [
        "typescript",
        "learn-gulp"
    ],
    "author": "hubiqing",
    "repository": "https://github.com/githbq/hbq-gulp.git",
    "engines": {
        "node": ">=7.6"
    },
    "license": "MIT"
}